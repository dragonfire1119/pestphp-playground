        "glorand/laravel-model-settings": "^4.0",
        "grimzy/laravel-mysql-spatial": "^4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "hashids/hashids": "^4.0",
        "hotmeteor/spectator": "^0.5.4",
        "hotrush/laravel-backup-telegram-notifications": "^1.0",
        "igaster/laravel-theme": "^2.0",
        "inertiajs/inertia-laravel": "^0.3.6",
        "itsgoingd/clockwork": "^5.0",
        "juanmiguelbesada/mjml-php": "^1.0",
        "larabug/larabug": "^2.3",
        "laracasts/utilities": "^3.1",
        "laravel-notification-channels/onesignal": "^2.0",
        "laravel-notification-channels/telegram": "^0.5.0",
        "laravel/cashier": "^12.9",
        "laravel/framework": "^8.56",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "^5.7",
        "laravel/legacy-factories": "^1.1",
        "laravel/nexmo-notification-channel": "^2.0",
        "laravel/passport": "^10.0",
        "laravel/scout": "^8.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-memory": "^1.0",
        "league/glide-laravel": "^1.0",
        "lorisleiva/laravel-actions": "^2.0",
        "maatwebsite/excel": "^3.1",
        "marcin-orlowski/laravel-api-response-builder": "^9.1",
        "matchish/laravel-scout-elasticsearch": "^4.0",
        "mpociot/teamwork": "dev-master",
        "nwidart/laravel-modules": "^7.2",
        "php-mime-mail-parser/php-mime-mail-parser": "^6.0",
        "predis/predis": "^1.1",
        "propaganistas/laravel-phone": "^4.2",
        "rennokki/laravel-eloquent-query-cache": "^2.6",
        "rlanvin/php-rrule": "^2.2",
        "santigarcor/laratrust": "6.3.*",
        "sentry/sentry-laravel": "2.2.*",
        "setasign/fpdi-tcpdf": "^2.3",
        "soundasleep/html2text": "~0.5",
        "sparkpost/sparkpost": "^2.2",
        "spatie/flysystem-dropbox": "^1.2",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-backup": "^6.13",
        "spatie/laravel-feed": "^2.7",
        "spatie/laravel-fractal": "^5.7",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-model-status": "^1.10",
        "spatie/laravel-ray": "^1.19",
        "spatie/laravel-schemaless-attributes": "^1.1",
        "spatie/laravel-sluggable": "2.6.*",
        "spatie/laravel-tags": "^2.1",
        "spatie/pdf-to-text": "^1.2",
        "vemcogroup/laravel-sparkpost-driver": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "filp/whoops": "^2.0",
        "imanghafoori/laravel-microscope": "^1.0",
        "johnkary/phpunit-speedtrap": "^4.0",
        "lukeraymonddowning/pest-plugin-money": "^1.0",
        "mockery/mockery": "^1.4.2",
        "nexusphp/tachycardia": "^1.3",
        "nunomaduro/collision": "^5.8",
        "nunomaduro/larastan": "^0.7.10",
        "nunomaduro/phpinsights": "^2.0",
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin": "^1.0",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pestphp/pest-plugin-parallel": "^0.3.0",
        "pestphp/pest-plugin-watch": "1.x-dev",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
